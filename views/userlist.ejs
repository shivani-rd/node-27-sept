<%- include('./base/header.ejs') %>
<div class="container">
    <div class="row">
        <h4 class="text-center my-3">User List</h4>
        <table class="table text-center">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Country Code</th>
                    <th>Mobile No</th>
                    <th>Email</th>
                    <th>Password</th>
                    <th>confirm Password</th>
                </tr>
                <% users.forEach((user)=>{ %>
                    <tr>
                        <td><%=user.firstName %></td>
                        <td><%=user.lastName %></td>
                        <td><%=user.countryCode %></td>
                        <td><%=user.mobileNo %></td>
                        <td><%=user.email %></td>
                        <td><%=user.password %></td>\
                        <td><%=user.confirmpassword %></td>
                      
                    </tr>
               <% }) %>
            </thead>
        </table>
    </div>
</div>
<%- include('./base/footer.ejs') %>